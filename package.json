{
    "name": "@badisi/latest-version",
    "version": "4.0.0",
    "description": "Get latest versions of packages",
    "homepage": "https://github.com/badisi/latest-version",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "main": "cjs/index.js",
    "types": "index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "require": "./cjs/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badisi/latest-version.git"
    },
    "keywords": [
        "npm",
        "check",
        "updates",
        "latest",
        "current",
        "next",
        "version",
        "pkg",
        "package",
        "package-json",
        "dependencies",
        "module"
    ],
    "scripts": {
        "lint": "eslint ./src/**/*.ts --ignore-pattern *.spec.ts",
        "test": "ts-node ./node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
        "build": "node ./build.mjs"
    },
    "dependencies": {
        "global-dirs": "^3.0.0",
        "registry-auth-token": "^5.0.1",
        "semver": "^7.3.7"
    },
    "devDependencies": {
        "@colors/colors": "^1.5.0",
        "@types/fs-extra": "^9.0.13",
        "@types/jasmine": "^4.0.3",
        "@types/node": "^18.0.3",
        "@types/registry-auth-token": "^4.2.1",
        "@types/rewire": "^2.5.28",
        "@types/semver": "^7.3.10",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "cpy": "^9.0.1",
        "esbuild": "^0.14.49",
        "eslint": "^8.19.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.3",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "^10.1.0",
        "jasmine": "^4.2.1",
        "jasmine-spec-reporter": "^7.0.0",
        "rewire": "^6.0.0",
        "ts-node": "^10.8.2",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">= 14"
    }
}
