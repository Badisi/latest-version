{
    "name": "@badisi/latest-version",
    "version": "7.0.0",
    "description": "Get latest versions of packages",
    "homepage": "https://github.com/badisi/latest-version",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "type": "commonjs",
    "main": "index.js",
    "typings": "index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "default": "./index.js",
            "require": "./index.js"
        },
        "./package.json": "./package.json"
    },
    "bin": {
        "latest-version": "./bin/latest-version",
        "lv": "./bin/latest-version"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badisi/latest-version.git"
    },
    "keywords": [
        "npm",
        "check",
        "updates",
        "latest",
        "current",
        "next",
        "version",
        "pkg",
        "package",
        "package-json",
        "dependencies",
        "module"
    ],
    "scripts": {
        "ncu": "npx npm-check-updates -i --format=group --packageFile '{,projects/**/}package.json'",
        "start": "ts-node --project=./tsconfig.json ./src/cli",
        "lint": "eslint ./src/**/*.ts --ignore-pattern *.spec.ts",
        "test": "ts-node ./node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
        "build": "node ./build.mjs"
    },
    "engines": {
        "node": ">= 18"
    },
    "dependencies": {
        "@colors/colors": "^1.6.0",
        "global-dirs": "^3.0.1",
        "ora": "5.4.1",
        "registry-auth-token": "^5.0.2",
        "semver": "^7.5.4"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.2",
        "@types/jasmine": "^5.1.1",
        "@types/node": "^20.8.7",
        "@types/registry-auth-token": "^4.2.2",
        "@types/rewire": "^2.5.29",
        "@types/semver": "^7.5.3",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "cpy": "^10.1.0",
        "esbuild": "^0.19.5",
        "eslint": "^8.51.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsdoc": "^46.8.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "^11.1.1",
        "jasmine": "^5.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "rewire": "^7.0.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    }
}
