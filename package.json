{
    "name": "@badisi/latest-version",
    "version": "2.1.1",
    "description": "Get latest versions of packages",
    "homepage": "https://github.com/badisi/latest-version",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
        ".": {
            "require": "./cjs/index.js",
            "import": "./esm/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badisi/latest-version.git"
    },
    "keywords": [
        "npm",
        "check",
        "updates",
        "latest",
        "current",
        "next",
        "version",
        "pkg",
        "package",
        "package-json",
        "dependencies",
        "module"
    ],
    "scripts": {
        "lint": "eslint ./src/**/*.ts --ignore-pattern *.spec.ts",
        "test": "ts-node ./node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
        "build": "node ./build.js",
        "all": "npm run lint && npm run test && npm run build"
    },
    "dependencies": {
        "global-dirs": "^3.0.0",
        "registry-auth-token": "^4.2.1",
        "semver": "^7.3.5"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/jasmine": "^3.10.3",
        "@types/node": "^17.0.16",
        "@types/registry-auth-token": "^4.2.1",
        "@types/rewire": "^2.5.28",
        "@types/semver": "^7.3.9",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "cpy": "^8.1.2",
        "eslint": "^8.8.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.8.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "^10.0.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-reporter": "^7.0.0",
        "rewire": "^6.0.0",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "engines": {
        "node": ">= 12"
    }
}
