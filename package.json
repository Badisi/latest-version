{
    "name": "@badisi/latest-version",
    "version": "2.0.2",
    "description": "Get latest versions of packages",
    "homepage": "https://github.com/Badisi/latest-version",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "main": "./index.js",
    "types": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/latest-version.git"
    },
    "keywords": [
        "npm",
        "check",
        "updates",
        "latest",
        "current",
        "next",
        "version",
        "pkg",
        "package",
        "package-json",
        "dependencies",
        "module"
    ],
    "scripts": {
        "lint": "eslint ./src/**/*.ts --ignore-pattern *.spec.ts",
        "test": "ts-node ./node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
        "build": "node ./build.js",
        "all": "npm run lint && npm run test && npm run build"
    },
    "dependencies": {
        "global-dirs": "^3.0.0",
        "registry-auth-token": "^4.2.1",
        "semver": "^7.3.5"
    },
    "devDependencies": {
        "@types/jasmine": "^3.7.7",
        "@types/node": "^15.12.4",
        "@types/registry-auth-token": "^4.2.0",
        "@types/rewire": "^2.5.28",
        "@types/semver": "^7.3.6",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "eslint": "^7.29.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.4.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "^10.0.0",
        "jasmine": "^3.7.0",
        "jasmine-spec-reporter": "^7.0.0",
        "rewire": "^5.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.4"
    }
}
